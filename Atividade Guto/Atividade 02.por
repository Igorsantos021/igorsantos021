programa {
  funcao inicio() {
    /* Considerando que todos os meses tenham 30 dias, calcular o total de dias de n meses
    informado pelo usuario. Exibir na tela o resultado do calculo com uma mensagem amigável.
    */
inteiro n, total_dias
escreva("Digite o numero de meses: ")
leia (n)
total_dias = (n * 30)
escreva ("O total de dias em ", n, " meses é ", total_dias, ".")
  }
}
