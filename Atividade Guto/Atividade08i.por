programa
{
	funcao inicio()
	{
		real A, B, C
 
		escreva("Insira o valor de A: ")
		leia(A)
 
		escreva("Insira o valor de B: ")
		leia(B)
 
		escreva("Insira o valor de C: ")
		leia(C)
 
		se (A + B < C) {
			escreva("A soma de A + B é menor que C")
		} senao {
			escreva("A soma de A + B não é menor que C")
		}
	}
}